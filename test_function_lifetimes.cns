fn longest(x: &string, y: &string): &string {
    if (x.length > y.length) {
        return x;
    } else {
        return y;
    }
}

fn invalid_lifetime(): &i32 {
    let x = 10;
    return &x; // error: returns a reference to a local variable
}

fn main(): i32 {
    let s1 = "hello";
    let s2 = "world";
    let l = longest(&s1, &s2);
    println(l);

    let i = invalid_lifetime();
    println(i);

    return 0;
}

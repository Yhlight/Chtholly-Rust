// Test cases for ownership and move semantics

// --- Valid cases ---

// 1. Primitive types (copy semantics)
let a: i32 = 10;
let b: i32 = a; // Valid: i32 is a copy type

// 2. Mutable primitive types
let mut c: i32 = 20;
c = 30; // Valid: c is mutable
let d: i32 = c; // Valid: c is copied

// --- Invalid cases ---

// 3. Moving a non-copy type
let x: string = "hello";
let y: string = x;
let z: string = x;

// 4. Using a moved variable
let s1: string = "world";
let s2: string = s1;
// let len: i32 = s1.length();

// 5. Assigning to an immutable variable
let imm: i32 = 5;
imm = 10;

// 6. Double move
let m1: string = "double";
let m2: string = m1;
let m3: string = m1;
